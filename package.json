{
  "name": "my-sentry-demo",
  "version": "1.0.0",
  "description": "My Sentry Demo with Express and TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts",
    "prestart": "npm run build"
  },
  "keywords": [
    "sentry",
    "express",
    "typescript"
  ],
  "author": "Kyaw Soe",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@sentry/node": "^10.20.0",
    "@sentry/profiling-node": "^10.20.0",
    "@sentry/tracing": "^7.120.4",
    "axios": "^1.12.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "nodemon": "^3.1.10",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
